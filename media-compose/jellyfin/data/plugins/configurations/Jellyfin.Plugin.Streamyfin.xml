<?xml version="1.0" encoding="utf-8"?>
<PluginConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Config>
    <notifications>
      <SessionStarted>
        <Enabled>true</Enabled>
        <RecentEventThreshold xsi:nil="true" />
      </SessionStarted>
      <PlaybackStarted>
        <Enabled>true</Enabled>
        <RecentEventThreshold xsi:nil="true" />
      </PlaybackStarted>
      <UserLockedOut>
        <Enabled>true</Enabled>
        <RecentEventThreshold xsi:nil="true" />
      </UserLockedOut>
      <ItemAdded>
        <Enabled>true</Enabled>
        <RecentEventThreshold xsi:nil="true" />
      </ItemAdded>
    </notifications>
    <settings>
      <home>
        <locked>false</locked>
        <value>
          <sections>
            <Section>
              <title>Continue Watching</title>
              <orientation>vertical</orientation>
              <items>
                <filters>
                  <ItemFilter>IsResumable</ItemFilter>
                </filters>
                <includeItemTypes>
                  <BaseItemKind>Episode</BaseItemKind>
                  <BaseItemKind>Movie</BaseItemKind>
                </includeItemTypes>
                <limit>25</limit>
              </items>
            </Section>
            <Section>
              <title>Nextup</title>
              <orientation>horizontal</orientation>
              <nextUp>
                <limit>25</limit>
                <enableResumable xsi:nil="true" />
                <enableRewatching xsi:nil="true" />
              </nextUp>
            </Section>
            <Section>
              <title>Recently Added</title>
              <orientation>vertical</orientation>
              <items>
                <sortBy>
                  <ItemSortBy>DateCreated</ItemSortBy>
                </sortBy>
                <sortOrder>
                  <SortOrder>Descending</SortOrder>
                </sortOrder>
                <includeItemTypes>
                  <BaseItemKind>Series</BaseItemKind>
                  <BaseItemKind>Movie</BaseItemKind>
                </includeItemTypes>
                <limit>25</limit>
              </items>
            </Section>
            <Section>
              <title>Favorites</title>
              <orientation>vertical</orientation>
              <items>
                <sortBy>
                  <ItemSortBy>Default</ItemSortBy>
                </sortBy>
                <sortOrder>
                  <SortOrder>Ascending</SortOrder>
                </sortOrder>
                <filters>
                  <ItemFilter>IsFavorite</ItemFilter>
                  <ItemFilter>IsUnplayed</ItemFilter>
                </filters>
                <includeItemTypes>
                  <BaseItemKind>Series</BaseItemKind>
                  <BaseItemKind>Movie</BaseItemKind>
                </includeItemTypes>
                <limit>25</limit>
              </items>
            </Section>
          </sections>
        </value>
      </home>
      <forwardSkipTime>
        <locked>false</locked>
        <value>30</value>
      </forwardSkipTime>
      <rewindSkipTime>
        <locked>false</locked>
        <value>15</value>
      </rewindSkipTime>
      <rememberAudioSelections>
        <locked>false</locked>
        <value>false</value>
      </rememberAudioSelections>
      <subtitleMode>
        <locked>false</locked>
        <value>Default</value>
      </subtitleMode>
      <rememberSubtitleSelections>
        <locked>false</locked>
        <value>false</value>
      </rememberSubtitleSelections>
      <subtitleSize>
        <locked>false</locked>
        <value>80</value>
      </subtitleSize>
      <autoRotate>
        <locked>false</locked>
        <value>true</value>
      </autoRotate>
      <defaultVideoOrientation>
        <locked>false</locked>
        <value>Default</value>
      </defaultVideoOrientation>
      <safeAreaInControlsEnabled>
        <locked>false</locked>
        <value>true</value>
      </safeAreaInControlsEnabled>
      <showCustomMenuLinks>
        <locked>false</locked>
        <value>false</value>
      </showCustomMenuLinks>
      <hiddenLibraries>
        <locked>false</locked>
        <value>
          <string>Enter library id(s)</string>
        </value>
      </hiddenLibraries>
      <disableHapticFeedback>
        <locked>false</locked>
        <value>false</value>
      </disableHapticFeedback>
      <downloadMethod>
        <locked>false</locked>
        <value>remux</value>
      </downloadMethod>
      <remuxConcurrentLimit>
        <locked>false</locked>
        <value>One</value>
      </remuxConcurrentLimit>
      <autoDownload>
        <locked>false</locked>
        <value>false</value>
      </autoDownload>
      <optimizedVersionsServerUrl>
        <locked>false</locked>
        <value>Enter optimized server url</value>
      </optimizedVersionsServerUrl>
      <jellyseerrServerUrl>
        <locked>false</locked>
        <value>Enter jellyseerr server url</value>
      </jellyseerrServerUrl>
      <searchEngine>
        <locked>false</locked>
        <value>Jellyfin</value>
      </searchEngine>
      <marlinServerUrl>
        <locked>false</locked>
        <value>Enter marlin server url</value>
      </marlinServerUrl>
      <libraryOptions>
        <locked>false</locked>
        <value>
          <display>list</display>
          <cardStyle>detailed</cardStyle>
          <imageStyle>cover</imageStyle>
          <showTitles>true</showTitles>
          <showStats>true</showStats>
        </value>
      </libraryOptions>
    </settings>
  </Config>
</PluginConfiguration>